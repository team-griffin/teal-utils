{
  "name": "@team-griffin/uteals",
  "version": "2.0.1",
  "description": "Utilities for Tealium",
  "main": "dist/cjs/uteals.js",
  "module": "dist/es/uteals.js",
  "repository": "git@github.com:team-griffin/uteals.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "transpile": "babel src -d lib --copy-files",
    "lint": "yarn lint:js && yarn flow",
    "lint:js": "eslint -c ./config/.eslintrc.js 'src/**/*.js'",
    "lint:js:fix": "eslint --fix -c ./config/.eslintrc.js 'src/**/*.js'",
    "flow": "flow check; test $? -eq 0 -o $? -eq 2",
    "build": "rollup --config ./rollup/config.js",
    "test": "cross-env NODE_ENV=test nyc mocha -- \"src/**/*.test.js\"",
    "test:debug": "cross-env NODE_ENV=test nyc mocha --inspect-brk -- \"src/**/*.test.js\"",
    "postmerge": "yarn"
  },
  "devDependencies": {
    "@team-griffin/eslint-config": "^3.0.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^8.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.13.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "flow-bin": "0.75.x",
    "mocha": "^4.0.1",
    "nyc": "^11.0.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "sinon": "^5.0.7"
  },
  "dependencies": {
    "browser-cookies": "^1.2.0",
    "ramda": "^0.26.1"
  }
}
